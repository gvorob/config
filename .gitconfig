[pretty]
  tree  =%w(90,0,8)%C(auto)%h %Cblue%<(8,trunc)%ae%Creset%C(auto)%d %s 
  tree2 =%w(90,0,8)%C(auto)%h %C(dim blue)(%cd) %Creset%C(dim green)%ae%Creset %C(auto)%d    %n %s
  cleanlog = %Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset
[user]
	email = janet.vorobyeva@gmail.com
[push]
	default = simple
[alias]
	tree = log --graph --pretty=tree --decorate --all --abbrev-commit --color
	tree2 = log --graph --pretty=tree2 --date=relative --all --color
  l = log --pretty=cleanlog
